{
  "name": "ui5-sankeyjs",
  "version": "0.1.0",
  "description": "Sankey Diagram controls for OpenUI 5",
  "repository": {
    "type": "git",
    "url": "https://github.com/uia4sap/ui5-sankeyjs.git"
  },
  "homepage": "https://github.com/uia4sap/ui5-sankeyjs",
  "ui5lab": {
    "namespace": "ui5.sankeyjs",
    "icon": "chart",
    "displayName": "ui5-sankeyjs"
  },
  "license": "Apache-2.0",
  "private": false,
  "scripts": {
    "prepare": "node prepare.js",
    "start": "ui5 serve --open ./test-resources/ui5lab/browser/index.html",
    "build": "rimraf dist && ui5 build",
    "release": "rimraf dist && ui5 build --exclude-task=createDebugFiles",
    "eslint": "eslint src/ui5/sankeyjs"
  },
  "dependencies": {
    "@openui5/sap.f": "1.58.4",
    "@openui5/sap.m": "1.58.4",
    "@openui5/sap.ui.core": "1.58.4",
    "@openui5/sap.ui.layout": "1.58.4",
    "@openui5/themelib_sap_belize": "1.58.4",
    "plotly.js": "^2.3.1"
  },
  "devDependencies": {
    "@ui5/cli": "^1.14.0",
    "eslint": "^5.2.0",
    "fs-extra": "^3.0.0",
    "ui5lab-browser": "1.0.0"
  }
}
